{
    "_version": "1.1.0",
	
	"start_url": "<startUrl>",
	
    "sap.app": {
        "_version": "1.1.0",
        "id": "sap.ui.demo.webtech",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}"
    },
    "sap.ui": {
        "technology": "UI5",
        "_version": "1.1.0",
        "icons": {
          "icon": "<icon>",
          "favIcon": "<favIcon>",
          "phone": "<phone>",
          "phone@2": "<phone@2>",
          "tablet": "<tablet>",
          "tablet@2": "<tablet@2>"
        },
        "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": false
        },
        "supportedThemes": ["sap_fiori_3"]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "sap.ui.demo.webtech.view.App",
            "type": "XML",
            "id": "app"
          },
          "dependencies": {
            "minUI5Version": "1.93",
            "libs": {
              "sap.ui.core": {},
              "sap.m": {}
            }
          },
          "models": {
            "i18n": {
              "type": "sap.ui.model.resource.ResourceModel",
              "settings": {
                "bundleName": "sap.ui.demo.webtech.i18n.i18n",
                "supportedLocales": [""],
                "fallbackLocale": ""
              }
            },
            "lecture": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "Lectures.json"
            }
          },
          "resources":{
            "css":[
              {
                "uri": "css/style.css"
              }
            ]
          }
    },

    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": "<uri>"
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": "<uri>"
    }
}